{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/www/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "modes": {
    "window": {
      "title": "TODOapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/www/dist/vite.svg",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "nativeBlockList": [
        "filesystem.*"
      ]
    }
  },
  "cli": {
    "binaryName": "todo-app",
    "resourcesPath": "/www/dist",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/www/public/neutralino.js",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0",
    "frontendLibrary": {
      "patchFile": "/www/index.html",
      "devUrl": "http://localhost:5173"
  }
  }
}