{
  "name": "autoclose-tags-plus",
  "displayName": "AutoClose Tags+",
  "description": "An extension that automatically closes tags with configurable indentation.",
  "version": "1.0.0",
  "publisher": "Giuseppe Gigliotti",
  "engines": {
    "vscode": "^1.50.0"
  },
  "main": "./extension.js",
  "categories": ["Other"],
  "activationEvents": ["onLanguage:html", "onLanguage:xml", "onLanguage:javascriptreact", "onLanguage:typescriptreact", "onLanguage:svelte", "onLanguage:xaml", "onLanguage:astro", "onLanguage:json"],
  "contributes": {
    "configuration": {
      "title": "AutoClose Tags+",
      "properties": {
        "autocloseTagsPlus.insertOnNewLine": {
          "type": "boolean",
          "default": true,
          "description": "Insert closing tag on a new line with indentation."
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/autoclose-tags-plus.git"
  },
  "scripts": {},
  "devDependencies": {},
  "license": "MIT"
}
